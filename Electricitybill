#include <stdio.h>

int main() {
    int units;
    float cost = 0;
    printf("Enter electricity units consumed: ");
    scanf("%d", &units);

    if (units <= 100) cost = units * 2;
    else if (units <= 300) cost = 100*2 + (units-100)*3;
    else cost = 100*2 + 200*3 + (units-300)*5;

    printf("Your estimated bill is: â‚¹%.2f\n", cost);
    return 0;
}
